{
  "Destructuring an object": {
    "prefix": [
      "dobj"
    ],
    "body": "const { $2 } = ${1:object};"
  },
  "Destructuring an array": {
    "prefix": [
      "darr"
    ],
    "body": "const [ $2 ] = ${1:array};"
  },
  "Arrow function with arguments": {
    "prefix": [
      "afn"
    ],
    "body": [
      "($1) => {",
      "\t$2",
      "}"
    ]
  },
  "Export all in a file": {
    "prefix": "expall",
    "body": [
      "export * from \"./$1\";"
    ]
  }
}
